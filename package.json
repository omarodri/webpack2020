{
    "name": "webpack-course",
    "version": "1.0.0",
    "description": "Webpack COruse in plz",
    "main": "index.js",
    "scripts": {
        "build": "webpack --entry ./index.js --output ./dist/bundle.js --mode development",
        "build:local": "webpack --mode production",
        "build:external": "webpack --config ./1.external/webpack.config.js",
        "build:multi": "webpack --config ./2.multi-entry-point/webpack.config.js",
        "build:css": "webpack --config ./3.css-style-loader/webpack.config.js",
        "build:plugins": "webpack --config ./4.plugins/webpack.config.js",
        "build:devserver": "webpack-dev-server --config ./5.webpack-dev-server/webpack.config.js -w",
        "build:babel": "webpack-dev-server --config ./6.babel/webpack.config.js -w",
        "build:react": "webpack-dev-server --config ./7.react/webpack.config.js -w"
    },
    "keywords": [
        "webpack"
    ],
    "author": "Omar Rodriguez",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "7.9.6",
        "@babel/plugin-transform-runtime": "7.9.6",
        "@babel/preset-env": "7.9.6",
        "@babel/preset-react": "7.9.4",
        "babel-loader": "8.1.0",
        "css-loader": "3.5.3",
        "html-webpack-plugin": "4.3.0",
        "mini-css-extract-plugin": "0.9.0",
        "style-loader": "1.2.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "3.10.3"
    },
    "dependencies": {
        "@babel/runtime": "7.9.6",
        "react": "16.13.1",
        "react-dom": "16.13.1"
    }
}